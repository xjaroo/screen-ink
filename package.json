{
  "name": "screen-ink",
  "version": "0.0.1",
  "description": "drawing on the screen by electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "startWithTransparentOption": "electron --enable-transparent-visuals .",
    "startWithAllOptions": "electron --enable-transparent-visuals --disable-gpu .",
    "build": "npx electron-builder -mwl",
    "build-mac": "npx electron-builder  --mac",
    "build-win": "npx electron-builder --win",
    "build-linux": "npx electron-builder --linux"
  },
  "keywords": [
    "Electron",
    "transparent",
    "tutorial",
    "transparency"
  ],
  "author": "xjaroo",
  "license": "MIT",
  "devDependencies": {
    "electron": "^27.0.1"
  },
  "dependencies": {
    "@electron/remote": "^2.0.12",
    "electron-localshortcut": "^3.2.1"
  }
}
